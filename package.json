{
  "name": "humanex-bot",
  "version": "5.0.0",
  "description": "HUMANEX BOT - Advanced RPA Automation System",
  "main": "backend/server.js",
  "scripts": {
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "backend": "nodemon backend/server.js",
    "frontend": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "start": "node backend/server.js",
    "install-all": "npm install && cd frontend && npm install"
  },
  "keywords": ["rpa", "automation", "bot", "playwright"],
  "author": "CODEWITHASAD",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.1",
    "playwright": "^1.40.1",
    "ws": "^8.16.0",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  }
}
